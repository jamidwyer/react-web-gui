{
  "extends": ["eslint:recommended", "airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }]
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "globals": {
    "fetch": true,
    "shallow": true,
    "render": true,
    "mount": true
  }
}
