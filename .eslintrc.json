{
  "extends": ["airbnb", "prettier", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "prettier/prettier": ["warn"]
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "fetch": true,
    "shallow": true,
    "render": true,
    "mount": true
  }
}
